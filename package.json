{
  "name": "@zanejs/vite-config",
  "version": "1.0.0",
  "description": "Out-of-the-box Vite configuration library supporting both application and library project types.",
  "license": "MIT",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "compile": "tsc -b ./tsconfig.esm.json ./tsconfig.types.json",
    "build:clean": "rm -rf ./dist",
    "build": "npm-run-all build:clean compile"
  },
  "files": [
    "dist",
    "README.md",
    "README.zh-CN.md"
  ],
  "main": "./dist/esm/index.js",
  "module": "./dist/esm/index.js",
  "types": "index.d.ts",
  "exports": {
    "./*": {
      "types": "./dist/types/*.d.ts",
      "import": "./dist/esm/*.js",
      "default": "./dist/esm/*.js"
    },
    "./package.json": "./package.json"
  },
  "devDependencies": {
    "@pnpm/workspace.read-manifest": "^1000.2.9",
    "@types/archiver": "^7.0.0",
    "@types/html-minifier-terser": "^7.0.2",
    "@types/node": "^25.0.8",
    "@vitejs/plugin-vue": "^6.0.3",
    "@vitejs/plugin-vue-jsx": "^5.1.3",
    "consola": "^3.4.2",
    "npm-run-all": "^4.1.5",
    "sass": "^1.97.2",
    "vite": "^7.3.1",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-dts": "^4.5.4",
    "vite-plugin-html": "^3.2.2",
    "vite-plugin-lazy-import": "^1.0.7",
    "vite-plugin-pwa": "^1.2.0",
    "vite-plugin-vue-devtools": "^8.0.5"
  },
  "dependencies": {
    "@intlify/unplugin-vue-i18n": "^11.0.3",
    "@jspm/generator": "^2.8.0",
    "@manypkg/get-packages": "^3.1.0",
    "archiver": "^7.0.1",
    "chalk": "^5.6.2",
    "cheerio": "^1.1.2",
    "dayjs": "^1.11.19",
    "get-port": "^7.1.0",
    "html-minifier-terser": "^7.2.0",
    "nitropack": "^2.13.1",
    "pkg-types": "^2.3.0",
    "rollup-plugin-visualizer": "^6.0.5"
  }
}
